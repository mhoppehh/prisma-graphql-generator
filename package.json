{
  "name": "prisma-generator-graphql-test",
  "description": "Generates a gql schema from prisma object",
  "version": "9.9.9",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-graphql-typedef": "dist/bin.js"
  },
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "pnpm build",
    "test": "npx jest",
    "prisma:generate:sdl": "prisma generate --generator=graphql_typedef --schema=./src/tests/__fixtures__/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@prisma/generator-helper": "^6.15.0",
    "@prisma/internals": "^6.15.0",
    "graphql": "^16.11.0",
    "handlebars": "^4.7.8",
    "prettier": "^3.6.2",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "jest": "^30.1.3",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
